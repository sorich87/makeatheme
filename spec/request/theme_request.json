{"name":"Test","regions":[{"name":"header","template":"<header id=\"branding\" role=\"banner\">\n  <div class=\"row\">\n    <hgroup class=\"eight columns\">\n      <h1 id=\"site-title\"><span><a href=\"{{ home_url }}\" title=\"\" rel=\"home\">{{{ site_title }}}</a></span></h1>\n      <h2 id=\"site-description\">{{{ site_description }}}</h2>\n    </hgroup>\n\n    {{{ search_form }}}\n  </div>\n\n  <div class=\"row\">\n    {{{ header_image }}}\n  </div>\n\n  <div class=\"row\">\n    {{{ menu }}}\n  </div>\n</header><!-- #branding -->\n"},{"name":"footer","template":"<footer id=\"colophon\" role=\"contentinfo\">\n  <div id=\"site-generator\" class=\"row\">\n    <div class=\"twelve columns\">\n      <a href=\"http://wordpress.org/\" title=\"Semantic Personal Publishing Platform\" rel=\"generator\">Proudly powered by WordPress</a>\n    </div>\n  </div>\n</footer><!-- #colophon -->\n"}],"templates":[{"name":"index","template":"<div class=\"row\" id=\"main\">\n  <div class=\"eight columns\" id=\"content\">\n    {{ article }}\n  </div>\n\n  <div class=\"four columns widget-area\" role=\"complementary\">\n    {{ sidebar }}\n  </div>\n</div>\n"},{"name":"page","template":"<div class=\"row\" id=\"main\">\n  <div class=\"eight columns\" id=\"content\">\n    {{ article }}\n  </div>\n\n  <div class=\"four columns widget-area\" role=\"complementary\">\n    {{ sidebar }}\n  </div>\n</div>\n"}]}
